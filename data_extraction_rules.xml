<?xml version="1.0" encoding="utf-8"?>
<!--
    Android 12+ data extraction rules used for cloud backup and device-to-device
    transfer. Mirrors the exclusions in backup_rules.xml.
-->
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="cache" />
        <exclude domain="external-cache" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="cache" />
        <exclude domain="external-cache" />
    </device-transfer>
</data-extraction-rules>
